{
  "name": "uilint-duplicates",
  "version": "0.1.1",
  "description": "Semantic code duplicate detection for React/TypeScript codebases",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./node": {
      "import": "./dist/node.js",
      "types": "./dist/node.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@typescript-eslint/typescript-estree": "^8.35.1",
    "glob": "^11.0.0",
    "xxhash-wasm": "^1.1.0"
  },
  "devDependencies": {
    "@types/glob": "^9.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "uilint-core": "workspace:*"
  }
}
