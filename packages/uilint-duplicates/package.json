{
  "name": "uilint-duplicates",
  "version": "0.2.53",
  "description": "Semantic code duplicate detection for React/TypeScript codebases",
  "author": "Peter Suggate",
  "repository": {
    "type": "git",
    "url": "https://github.com/peter-suggate/uilint.git",
    "directory": "packages/uilint-duplicates"
  },
  "homepage": "https://github.com/peter-suggate/uilint#readme",
  "bugs": {
    "url": "https://github.com/peter-suggate/uilint/issues"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "node": [
        "./dist/node.d.ts"
      ],
      "*": [
        "./dist/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./node": {
      "types": "./dist/node.d.ts",
      "import": "./dist/node.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@typescript-eslint/typescript-estree": "^8.35.1",
    "glob": "^11.0.0",
    "xxhash-wasm": "^1.1.0"
  },
  "devDependencies": {
    "@types/glob": "^9.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "uilint-core": "workspace:*"
  },
  "keywords": [
    "duplicate-detection",
    "code-analysis",
    "react",
    "typescript",
    "lint",
    "ui"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
