{
  "name": "uilint-react",
  "version": "0.2.62",
  "description": "React component for AI-powered UI consistency checking",
  "author": "Peter Suggate",
  "repository": {
    "type": "git",
    "url": "https://github.com/peter-suggate/uilint.git",
    "directory": "packages/uilint-react"
  },
  "homepage": "https://github.com/peter-suggate/uilint#readme",
  "bugs": {
    "url": "https://github.com/peter-suggate/uilint/issues"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./node": {
      "types": "./dist/node.d.ts",
      "import": "./dist/node.js"
    },
    "./devtools": {
      "types": "./dist/devtools-types.d.ts",
      "import": "./dist/devtools.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "vite build && vite build -c vite.devtools.config.ts && cp src/devtools-types.d.ts dist/",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "test -f coverage/coverage-final.json || vitest run --coverage; eslint src/",
    "lint:uilint": "npx uilint analyze --input-file src/components/ui-lint/toolbar/index.tsx --styleguide-path ./.uilint/styleguide.md",
    "prepare": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.4.0",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-slot": "^1.2.4",
    "class-variance-authority": "^0.7.1",
    "html-to-image": "^1.11.13",
    "lucide-react": "^0.562.0",
    "react-use-measure": "^2.1.7",
    "tw-animate-css": "^1.4.0",
    "uilint-core": "workspace:*",
    "uilint-eslint": "workspace:*",
    "zustand": "^5.0.10"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.1",
    "@types/jsdom": "^27.0.0",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.18",
    "clsx": "^2.1.1",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^17.0.0",
    "jiti": "^2.6.1",
    "jsdom": "^27.4.0",
    "motion": "^12.26.2",
    "postcss": "^8.5.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.17"
  },
  "keywords": [
    "react",
    "ui",
    "lint",
    "consistency",
    "design-system",
    "ai",
    "llm"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
