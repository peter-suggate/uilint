{
  "name": "uilint-react",
  "version": "0.2.2",
  "description": "React component for AI-powered UI consistency checking",
  "author": "Peter Suggate",
  "repository": {
    "type": "git",
    "url": "https://github.com/peter-suggate/uilint.git",
    "directory": "packages/uilint-react"
  },
  "homepage": "https://github.com/peter-suggate/uilint#readme",
  "bugs": {
    "url": "https://github.com/peter-suggate/uilint/issues"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./node": {
      "types": "./dist/node.d.ts",
      "import": "./dist/node.js"
    },
    "./devtools": {
      "import": "./dist/devtools.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "vite build && vite build -c vite.devtools.config.ts",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:uilint": "npx uilint analyze --input-file src/components/ui-lint/toolbar/index.tsx --styleguide-path ./.uilint/styleguide.md",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.4.0",
    "@radix-ui/react-slot": "^1.2.4",
    "class-variance-authority": "^0.7.0",
    "html-to-image": "^1.11.11",
    "lucide-react": "^0.462.0",
    "react-use-measure": "^2.1.7",
    "tw-animate-css": "^1.4.0",
    "uilint-core": "workspace:*",
    "uilint-eslint": "^0.2.1",
    "zustand": "^5.0.5"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/jsdom": "^21.1.7",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.0.0",
    "clsx": "^2.1.1",
    "eslint": "^9.28.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^17.0.0",
    "jiti": "^2.5.0",
    "jsdom": "^26.1.0",
    "motion": "^12.25.0",
    "postcss": "^8.5.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^4.0.16"
  },
  "keywords": [
    "react",
    "ui",
    "lint",
    "consistency",
    "design-system",
    "ai",
    "llm"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
