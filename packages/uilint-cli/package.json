{
  "name": "uilint-cli",
  "version": "0.1.23",
  "description": "CLI for UILint - AI-powered UI consistency checking",
  "author": "Peter Suggate",
  "repository": {
    "type": "git",
    "url": "https://github.com/peter-suggate/uilint.git",
    "directory": "packages/uilint-cli"
  },
  "homepage": "https://github.com/peter-suggate/uilint#readme",
  "bugs": {
    "url": "https://github.com/peter-suggate/uilint/issues"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "uilint": "./dist/index.js",
    "uilint-cli": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@clack/prompts": "^0.10.0",
    "commander": "^14.0.2",
    "chalk": "^5.6.2",
    "picocolors": "^1.1.1",
    "uilint-core": "^0.1.23"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "cli",
    "ui",
    "lint",
    "consistency",
    "design-system",
    "ai"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
