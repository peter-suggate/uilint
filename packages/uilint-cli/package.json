{
  "name": "uilint-cli",
  "version": "0.1.0",
  "description": "CLI for UILint - AI-powered UI consistency checking",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "uilint": "./dist/index.js",
    "uilint-cli": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "commander": "^14.0.2",
    "chalk": "^5.6.2",
    "ora": "^9.0.0",
    "uilint-core": "^0.1.0"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "cli",
    "ui",
    "lint",
    "consistency",
    "design-system",
    "ai"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
