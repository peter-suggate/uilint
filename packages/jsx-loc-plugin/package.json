{
  "name": "jsx-loc-plugin",
  "version": "0.1.0",
  "private": true,
  "description": "Next.js plugin that adds data-loc attributes to JSX elements (Next.js 16 compatible)",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "@babel/parser": "^7.24.0",
    "magic-string": "^0.30.10"
  },
  "devDependencies": {
    "next": "^16.1.1",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "next": ">=14.0.0"
  }
}
