{
  "name": "jsx-loc-plugin",
  "version": "0.2.31",
  "description": "Next.js plugin that adds data-loc attributes to JSX elements for UILint inspection",
  "author": "Peter Suggate",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/peter-suggate/uilint.git",
    "directory": "packages/jsx-loc-plugin"
  },
  "homepage": "https://github.com/peter-suggate/uilint#readme",
  "bugs": {
    "url": "https://github.com/peter-suggate/uilint/issues"
  },
  "keywords": [
    "next",
    "nextjs",
    "jsx",
    "uilint",
    "plugin",
    "webpack",
    "turbopack"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "@babel/parser": "^7.24.0",
    "magic-string": "^0.30.10"
  },
  "devDependencies": {
    "next": "^16.1.1",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.0"
  },
  "peerDependencies": {
    "next": ">=14.0.0",
    "vite": ">=5.0.0"
  }
}
