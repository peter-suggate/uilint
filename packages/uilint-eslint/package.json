{
  "name": "uilint-eslint",
  "version": "0.1.21",
  "description": "ESLint plugin for UILint - AI-powered UI consistency checking",
  "author": "Peter Suggate",
  "repository": {
    "type": "git",
    "url": "https://github.com/peter-suggate/uilint.git",
    "directory": "packages/uilint-eslint"
  },
  "homepage": "https://github.com/peter-suggate/uilint#readme",
  "bugs": {
    "url": "https://github.com/peter-suggate/uilint/issues"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "generate:index": "node --import tsx scripts/generate-index.ts",
    "build": "pnpm run generate:index && tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@typescript-eslint/typescript-estree": "^8.35.1",
    "@typescript-eslint/utils": "^8.35.1",
    "oxc-resolver": "^11.0.0",
    "uilint-core": "^0.1.51",
    "xxhash-wasm": "^1.1.0"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.16.0",
    "@typescript-eslint/rule-tester": "^8.35.1",
    "eslint": "^9.28.0",
    "tsx": "^4.21.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "ui",
    "lint",
    "consistency",
    "design-system",
    "ai",
    "llm",
    "tailwind"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
